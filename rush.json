{
  "rushVersion": "5.100.1",
  "yarnVersion": "1.9.4",
  "nodeSupportedVersionRange": ">=14.15.0 <15.0.0 || >=16.13.0 <17.0.0 || >=18.15.0 <19.0.0",
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 3,
  "approvedPackagesPolicy": {
    "reviewCategories": ["packages", "internal"],
    "ignoredNpmScopes": ["@types"]
  },
  "gitPolicy": {},
  "repository": {
    "url": "https://github.com/michael-mir/rush-example"
  },
  "ensureConsistentVersions": true,
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "projects": [
    {
      "packageName": "@packages/constants",
      "projectFolder": "packages/constants",
      "reviewCategory": "packages",
      "tags": ["@packages/constants"]
    },
    {
      "packageName": "@packages/tools",
      "projectFolder": "packages/tools",
      "reviewCategory": "packages",
      "tags": ["@packages/tools"]
    }
  ]
}
